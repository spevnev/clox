var arr = Array(4, nil);
for (var i = 0;i < length(arr);i = i + 1) {
    arr[i] = Array(3, nil);
    for (var j = 0;j < length(arr[0]);j = j + 1) {
        arr[i][j] = Array(2, 0);
        for (var k = 0;k < length(arr[0][0]);k = k + 1) {
            arr[i][j][k] = i + j + k;
        }
    }
}
print arr; // [[[0, 1], [1, 2], [2, 3]], [[1, 2], [2, 3], [3, 4]], [[2, 3], [3, 4], [4, 5]], [[3, 4], [4, 5], [5, 6]]]
